void setup() {
  //pinmode output 
  for(int i = 2 ; i < 18 ; i++)
  {
    pinMode(i, OUTPUT);
  }

    //column pin low 
  for(int i = 2 ; i < 10 ; i++)
  {
    digitalWrite(i, HIGH);
  }

  
  //-------------------
  Serial.begin(9600);


}

//col - h - off
//row - l - off

int count = 10;
int num[2] = {0x23, 0x37};

void loop() {
  //------ all row off
  for(int i = 0 ; i < 10 ; i++)
  {
    digitalWrite(i+10, LOW);
  }

//---------- data col
  for(int i = 0 ; i < 8 ; i++)
  {
    if(num[count-2] & (0x80>>i))
    {
      digitalWrite(2 + i, LOW);
    }
    else
    {
      digitalWrite(2 + i, HIGH);
    }
  }

//----------- row high
  digitalWrite(count, HIGH);
  delay(10);

  
  //------ all row off
  for(int i = 0 ; i < 10 ; i++)
  {
    digitalWrite(i+10, LOW);
  }


  
  count++;
  if(count > 11)
  {
    count = 10;
  }
  
}
